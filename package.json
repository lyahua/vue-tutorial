{
  "name": "vue-tutorial",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "ttt": "node ./scripts/index.js",
    "start": "npm-run-all ttt && (set NODE_OPTIONS=--openssl-legacy-provider && (npm run serve)) "
  },
  "dependencies": {
    "ant-design-vue": "^1.7.6",
    "axios": "^0.23.0",
    "core-js": "^3.6.5",
    "element-ui": "^2.15.3",
    "moment": "^2.29.1",
    "vue": "^2.6.11",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2",
    "vxe-table": "^3.6.13",
    "xe-utils": "^3.5.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "mockjs": "^1.1.0",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^10.4.1",
    "style-resources-loader": "^1.5.0",
    "vue-cli-plugin-style-resources-loader": "^0.1.5",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": false,
    "env": {
      "node": false
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-debugger": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
